<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="srio_rest_upload.upload_manager.class">SRIO\RestUploadBundle\Upload\UploadManager</parameter>
    </parameters>

    <services>
        <service id="srio_rest_upload.upload_manager" class="%srio_rest_upload.upload_manager.class%">
            <argument>%srio_rest_upload.configuration%</argument>
        </service>
    </services>
</container>
